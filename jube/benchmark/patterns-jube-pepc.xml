<patterns>

    <parm name="walltime" unit="s" mode="line,last"       type="float">ttot\s+$patfp</parm>
    <parm name="t_loop"   unit="s" mode="line,statistics" type="float">ttot\s+$patfp</parm>

    <parm name="time_pre"   unit="s" mode="line,statistics" type="float">pepc timing - pre:\s+$patfp</parm>
    <parm name="time_inner" unit="s" mode="line,statistics" type="float">pepc timing - inner:\s+$patfp</parm>
    <parm name="time_post"  unit="s" mode="line,statistics" type="float">pepc timing - post:\s+$patfp</parm>
    <parm name="time_total" unit="s" mode="line,statistics" type="float">pepc timing - total:\s+$patfp</parm>

    <parm name="IOperf" unit="MB/s" mode="line,statistics" type="float">benchmarking: rank $patnint io performance  $patfp MB/s</parm>
    <parm name="IOopen" unit="s" mode="line,statistics" type="float">benchmarking: rank $patnint time to open file  $patfp s</parm>
    <parm name="IOwrite" unit="s" mode="line,statistics" type="float">benchmarking: rank $patnint time to write file  $patfp s</parm>
    <parm name="IOclose" unit="s" mode="line,statistics" type="float">benchmarking: rank $patnint time to close file  $patfp s</parm>


</patterns>
