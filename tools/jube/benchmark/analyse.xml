<analyzer>

<!-- ******************************************************************************** -->
<!-- Input is stdout and stderr of benchmark run -->

<analyse cname="IBM-BGP-Babel">
   <includepattern file="./patterns-jube-pepc.xml" />
</analyse>

<analyse cname="Intel-Nehalem-JuRoPA">
   <includepattern file="./patterns-jube-pepc.xml" />
</analyse>

<analyse cname="IBM-BGP-Jugene">
   <precommand>(cd $outdir; bash collectData.sh)</precommand>
   
   <input addfiles="$subdir/IHPCT.log $subdir/GPROF.log" />

   <includepattern file="./patterns-jube-pepc.xml" /> 
   <includepattern file="./patterns-ihpct-pepc.xml" /> 
   <includepattern file="./patterns-gprof-pepc.xml" /> 
</analyse>

<analyse cname="IBM-SP6-Huygens">
