</analyse>

<analyse cname="Cray-XT4-Louhi">
   <precommand>(cd $outdir; bash collectData.sh)</precommand>
   
   <input addfiles="$subdir/IHPCT.log $subdir/GPROF.log $subdir/CRAYPAT.log" />

   <includepattern file="./patterns-jube-pepc.xml" /> 
   <includepattern file="./patterns-ihpct-pepc.xml" /> 
   <includepattern file="./patterns-gprof-pepc.xml" /> 
